<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:PokeLike.MVVM.View"
                    xmlns:vm="clr-namespace:PokeLike.MVVM.ViewModel">


    <DataTemplate DataType="{x:Type vm:MainViewVM}">
        <view:MainView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LoginViewVM}">
        <view:LoginView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SettingsViewVM}">
        <view:SettingsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:GameViewVM}">
        <view:GameView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:PokeListViewVM}">
        <view:PokeListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SpellListViewVM}">
        <view:SpellListView />
    </DataTemplate>
    <!--DataTemplate DataType="{x:Type vm:CreatePlayerViewModel}">
        <view:CreatePlayerDialog />
    </DataTemplate-->
</ResourceDictionary>